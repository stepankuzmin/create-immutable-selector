{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["import"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "max-len": [
      "error",
      { "code": 80, "ignoreStrings": true, "ignoreUrls": true }
    ],
    "quotes": ["error", "single"]
  }
}
